<?php
   $host = 'localhost';
   $user = 'username';
   $password = 'password';
   $dbname = 'database_name';
   
   // Create connection
   $conn = mysqli_connect($host, $user, $password, $dbname);
   
   // Check connection
   if (!$conn) {
       die("Connection failed: " . mysqli_connect_error());
   }
   echo "Connected successfully";
?>
