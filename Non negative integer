<?php
function haveSameLastDigit($num1, $num2) {
    return ($num1 >= 0 && $num2 >= 0) && (abs($num1 % 10) === abs($num2 % 10));
}

$result = haveSameLastDigit(25, 15);
if ($result) {
    echo "The two numbers have the same last digit";
} else {
    echo "The two numbers do not have the same last digit";
}
?>
